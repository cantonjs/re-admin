{
	"presets": [
		"react",
		["es2015"],
		"stage-0-without-async",
	],
	"plugins": [
		"transform-decorators-legacy",
	],
	"env": {
		"dev": {
			"presets": [
				["es2015", { "modules": false }],
				"stage-0",
			],
			"plugins": [
				"react-hot-loader/babel",
				"transform-decorators-legacy",
				"lodash",
				["import", {
					"libraryName": "antd",
					"style": true,
				}],
			],
		},
		"build": {
			"presets": [
				["es2015", { "modules": false }],
				"stage-0",
			],
			"plugins": [
				"transform-decorators-legacy",
				"lodash",
			],
		},
	},
}
